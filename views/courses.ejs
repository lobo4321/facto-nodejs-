<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="author" content="www.frebsite.nl" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no"
    />

    <title>LearnUp - Online Course & Education HTML Template</title>

    <!-- Custom CSS -->
    <link href="css/styles.css" rel="stylesheet" />

    <!-- Custom Color Option -->
    <link href="css/colors.css" rel="stylesheet" />
  </head>

  <body class="red-skin">
    <!-- ============================================================== -->
    <!-- Preloader - style you can find in spinners.css -->
    <!-- ============================================================== -->

    <!-- ============================================================== -->
    <!-- Main wrapper - style you can find in pages.scss -->
    <!-- ============================================================== -->
    <div id="main-wrapper">
      <!-- ============================================================== -->
      <!-- Top header  -->
      <!-- ============================================================== -->
      <!-- Start Navigation -->
      <%- include('parts/page-header') %>
      <!-- End Navigation -->
      <div class="clearfix"></div>
      <!-- ============================================================== -->
      <!-- Top header  -->
      <!-- ============================================================== -->

      <!-- ============================ Page Title Start================================== -->
      <%- include('parts/page-title', {title: 'Cursos e Vídeos Gratuitos',
      subtitle: 'Cursos'}) %>
      <!-- ============================ Page Title End ================================== -->

      <!-- ============================ Full Width Courses  ================================== -->
      <section class="pt-0">
        <div class="container">
          <!-- Onclick Sidebar -->
          <div class="row">
            <div class="col-md-12 col-lg-12">
              <div id="filter-sidebar" class="filter-sidebar">
                <div class="filt-head">
                  
                  <a
                    style="margin-left: auto;"
                    href="javascript:void(0)"
                    class="closebtn"
                    onclick="closeNav()"
                    > <i class="ti-close"></i
                  ></a>
                </div>
                <ul id="terminal1">
                <div class="show-hide-sidebar">
                  <div id="myBtnContainer">
                    <li class="sidebar-item">
                    <button
                      class="btn filter_open tag "

                      
                      onclick="closeNav()"
                    >
                      Mostrar todos
                    </button>
                  </li>
                    <li class="sidebar-item" >
                    <button
                      class="btn filter_open tag"

                      onclick="filterSelection('barbeiro');closeNav();change_urlBarber(); "
                      id="barbeiro"
                      value="Barbeiro"
                    
                    >
                      Barbeiro
                    </button>
                  </li>
                  <li class="sidebar-item">
                    <button
                      class="btn filter_open tag"
                      onclick="closeNav()"
                      
                      id="cabeleireiro"
                    >
                      Cabeleireiro
                    </button>
                  </li>
                  
                  <li class="sidebar-item">
                    <button
                      class="btn filter_open tag"
                      onclick="closeNav()"

                      id="designerCilios"
                    >
                      Designer Sobrancelhas
                    </button>
                  </li>
                 
                  
                  <li class="sidebar-item"> 
                    <button
                      class="btn filter_open tag"
                      onclick="closeNav()"

                      
                      id="manicure"
                    >
                      Manicure
                    </button>
                  </li>
                  <li class="sidebar-item">
                    <button
                      class="btn filter_open tag"
                      onclick="closeNav()"

                      
                      id="maquiador"
                    >
                      Maquiador
                    </button>
                  </li>
                  <li class="sidebar-item">
                    <button
                      class="btn filter_open tag"
                      onclick="closeNav()"

                      
                      id="tatuador"
                    >
                      Tatuador
                    </button>
                  </li>
                  </div>
                </div>
              </ul>
              </div>
            </div>

            <!-- Row -->
            <div class="row">
              <div class="col-lg-12 col-md-12 col-sm-12">
                <!-- Row -->
                <div class="row align-items-center mb-3" id="data-courses-length" style="flex-direction: row-reverse;">
                  <div class="col-lg-6 col-md-6 col-sm-12" style="text-align: end;" >
                    Encontramos <strong id="countCourses"> 142 </strong> cursos para você
                  </div>
                  <div class="col-lg-6 col-md-6 col-sm-12 ordering">
                    <div class="filter_wraps">
                      <div class="dl">
                        <div id="main2">
                          <a
                            href="javascript:void(0)"
                            class="btn btn-theme arrow-btn filter_open"
                            onclick="openNav()"
                            id="filterCourses"
                            >Filtrar<span
                              
                                
                              ><img src="/img/filter.svg" alt="" style="width: 35px; height: px;"></span
                          ></a>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <!-- /Row -->

                <div class="row containerCourses" id="data-courses"></div>
              </div>

              
            </div>
           
          </div>
           <!-- Row -->
           <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
              <!-- Pagination -->

            </div>
          </div>
          <!-- /Row -->
          </div>
          <!-- Row -->
        </div>
      </section>
      <!-- ============================ Full Width Courses End ================================== -->

      <!-- ============================== Start Newsletter ================================== -->
      <%- include('parts/page-newsletter') %>
      <!-- ================================= End Newsletter =============================== -->

      <!-- ============================ Footer Start ================================== -->
      <%- include('parts/page-footer') %>
      <!-- ============================ Footer End ================================== -->

      


      <a id="back2Top" class="top-scroll" title="Back to top" href="#"
        ><i class="ti-arrow-up"></i
      ></a>
    </div>
    <!-- ============================================================== -->
    <!-- End Wrapper -->
    <!-- ============================================================== -->

    <!-- ============================================================== -->
    <!-- All Jquery -->
    <!-- ============================================================== -->
    <script src="js/jquery.min.js"></script>
    <script src="js/popper.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/select2.min.js"></script>
    <script src="js/slick.js"></script>
    <script src="js/jquery.counterup.min.js"></script>
    <script src="js/counterup.min.js"></script>
    <script src="js/custom.js"></script>
    <script src="js/coursesAdd.js"></script>
    <!-- ============================================================== -->
    <!-- This page plugins -->
    <!-- ============================================================== -->
    <script>
      function openNav() {
        document.getElementById("filter-sidebar").style.width = "320px";
      }

      function closeNav() {
        document.getElementById("filter-sidebar").style.width = "0";
      }
    </script>

    <script src="js/index.js"></script>
  </body>
</html>

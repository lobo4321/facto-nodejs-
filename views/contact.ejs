      <!-- ============================ Agency List Start ================================== -->
      <section class="bg-light">
        <div class="container">
          <!-- row Start -->
          <div class="row">
            <div class="col-lg-8 col-md-7">
              <div class="prc_wrap">
                <div class="prc_wrap_header">
                  <h4 class="property_block_title" style="text-transform: none">
                    Preencha o formulário
                  </h4>
                </div>
                <form id="form" action="webdorm.php" method="POST">
                <div class="prc_wrap-body">
                  <div class="row">
                    <div class="col-lg-6 col-md-12"  >
                      <div class="form-group" >
                        <label >Nome Completo</label>
                        <input 
                          id="fname"
                          name="fname"
                          type="text"
                          class="form-control simple "
                          
                        />
                      </div>
                    </div>
                    <div class="col-lg-6 col-md-12">
                      <div class="form-group">
                        <label>E-mail</label>
                        <input

                          id="femail"
                          Name="Email"
                          type="email"
                          class="form-control simple "

                        />
                      </div>
                    </div>
                  </div>

                  <div class="form-group">
                    <label>Assunto</label>
                    <input

                      id="fsubject"
                      name="Subject"
                      type="text"
                      class="form-control simple"

                    />
                  </div>

                  <div class="form-group">
                    <label>Mensagem</label>
                    <input id="fmessage" class="form-control simple"/> 
                  </div>

                  <div class="form-group">
                    <button
                      class="btn btn-theme"
                      type="submit"
                      id="submitMessage"
                      style="border-radius: 4px"
                    >
                      Enviar mensagem!
                    </button>
                  </div>
                </div>
              </div>
            </form>
            </div>

